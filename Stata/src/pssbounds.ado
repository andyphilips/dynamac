		di in smcl in gr	 "1% critical value " in y  _col(21)  %10.3f  `t_01_0'   _col(37)       %10.3f `t_01_1'
		di 
		di in smcl in gr	 "t-stat. =" in y %10.3f `tstat'	
	}
}
